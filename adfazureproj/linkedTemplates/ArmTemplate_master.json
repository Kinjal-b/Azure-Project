{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adfazureproj"
		},
		"AzureDataLake_LS_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'AzureDataLake_LS'"
		},
		"S3_LS_secretAccessKey": {
			"type": "secureString",
			"metadata": "Secure string for 'secretAccessKey' of 'S3_LS'"
		},
		"SQL_DB_DS_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'SQL_DB_DS'"
		},
		"AzureDataLake_LS_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://azurestorageproj.dfs.core.windows.net/"
		},
		"AzureKeyVault1_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://azurekeyvaultproj.vault.azure.net/"
		},
		"S3_LS_properties_typeProperties_accessKeyId": {
			"type": "string",
			"defaultValue": "AKIA6ODVAK672ULETVDG"
		},
		"Storage_Trigger_forJson_properties_typeProperties_scope": {
			"type": "string",
			"defaultValue": "/subscriptions/dc1b6224-8a23-493f-9d6d-4d9ae9ed0395/resourceGroups/project1/providers/Microsoft.Storage/storageAccounts/azurestorageproj"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adfazureproj_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureDataLake_LS_accountKey": {
						"value": "[parameters('AzureDataLake_LS_accountKey')]"
					},
					"S3_LS_secretAccessKey": {
						"value": "[parameters('S3_LS_secretAccessKey')]"
					},
					"SQL_DB_DS_connectionString": {
						"value": "[parameters('SQL_DB_DS_connectionString')]"
					},
					"AzureDataLake_LS_properties_typeProperties_url": {
						"value": "[parameters('AzureDataLake_LS_properties_typeProperties_url')]"
					},
					"AzureKeyVault1_properties_typeProperties_baseUrl": {
						"value": "[parameters('AzureKeyVault1_properties_typeProperties_baseUrl')]"
					},
					"S3_LS_properties_typeProperties_accessKeyId": {
						"value": "[parameters('S3_LS_properties_typeProperties_accessKeyId')]"
					},
					"Storage_Trigger_forJson_properties_typeProperties_scope": {
						"value": "[parameters('Storage_Trigger_forJson_properties_typeProperties_scope')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}